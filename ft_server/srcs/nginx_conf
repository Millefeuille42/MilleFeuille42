server {
   listen 80;
   listen [::]:80;

   root /var/www/html;

   location / {
        index index.php;
   }

   location /pma {
        index index.php;
   }

   location ~ \.php$ {
      include snippets/fastcgi-php.conf;
      fastcgi_pass unix:/var/run/php/php7.3-fpm.sock;
   }

   location ~* \.(css|gif|ico|jpeg|jpg|js|png)$ {
       expires max;
       log_not_found off;
   }

}